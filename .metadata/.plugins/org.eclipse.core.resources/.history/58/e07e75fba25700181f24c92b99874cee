package hr.fer.zemris.java.gui.layouts;

/**
 * Represents row and column position in layout.
 * @author tin
 *
 */
public class RCPosition {
  private int row;
  private int column;
  
  public RCPosition(int row, int column) {
    this.row = row;
    this.column = column;
  }

  public int getRow() {
    return row;
  }

  public int getColumn() {
    return column;
  }
  
  
}
