package hr.fer.zemris.java.gui.charts;

/**
 * Represents one bar chart bar. X represents bar index starting at 1. Y
 * represents bar height.
 * 
 * @author tin
 *
 */
public class XYValue {
  private int x;
  private int y;

  public XYValue(int x, int y) {
    super();
    this.x = x;
    this.y = y;
  }

  public int getX() {
    return x;
  }

  public int getY() {
    return y;
  }

}
